	map				"+alienbase"
	params			("small1" "small2")
	description		"_Alien Base"
	storyrelated	true
	maxaliens		30
	multiplayer		true
	teams			2
	gametypes (
		fight1on1
	)
	missionbriefing		"*msgid:mission_briefing_alienbase"
	victorycondition	"*msgid:victory_condition_alienbase"
	onwin			"cp_add_researchable rs_alien_psi_device"
}

