// =======================
// FIRE FIELD
// ======================

particle firefield
{
	init
	{
		v		"0 0 0"
		tps		20.0
	}
	think
	{
		spawn   burnFireFlame
		spawn	firefield_fire
	}
}

particle firefield_fire
{
	init
	{
		image  "sfx/fireball"
		blend  add
		style  facing
		thinkfade  out
		rand   float 0.3		// 1/lifetime
		add    float 0.5
		pop    *tps
		crand  vector "14 14 0"	// variance position
		add    *s
		pop    *s
		rand  vector "8 8 35"	// variance velocity
		add    *v
		pop    *v
		physics false
	}
	run
	{
		push   pos "4 4"		// change in size
		mul    *t
		add    pos "14 14"		// size to start with
		pop    *size
	}
	think { kill }
}

