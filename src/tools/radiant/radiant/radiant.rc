#include <winresrc.h>

#ifdef PATH_UFORADIANTICON_FILE
# define STR_PATH_UFORADIANTICON_FILE PATH_UFORADIANTICON_FILE
#else
# define STR_PATH_UFORADIANTICON_FILE "radiant.ico"
#endif

101 ICON DISCARDABLE STR_PATH_UFORADIANTICON_FILE

VS_VERSION_INFO VERSIONINFO
