			VALUE "LegalCopyright", "Copyright (C) 2002-2014"
			VALUE "OriginalFilename", "uforadiant.exe"
			VALUE "ProductName", "UFORadiant"
			VALUE "ProductVersion", "Release"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x400, 1200
	END
END
